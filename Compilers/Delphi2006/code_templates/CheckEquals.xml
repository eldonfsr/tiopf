<?xml version="1.0" encoding="utf-8" ?>
<codetemplate	xmlns="http://schemas.borland.com/Delphi/2005/codetemplates"
				version="1.0.0">
	<template name="ce" invoke="auto">
		<point name="expected">
			<hint>
			   Expected value
			</hint>
			<text>
				Expected
			</text>
		</point>
		<point name="actual">
			<hint>
			   Actual value
			</hint>
			<text>
				Actual
			</text>
		</point>
		<description>
			CheckEquals(Expected, Actual);
		</description>
		<author>
			Peter Hinrichsen
		</author>
		<code language="Delphi" context="methodbody" delimiter="|"><![CDATA[CheckEquals(|expected|, |actual|);]]>
		</code>
	</template>
</codetemplate>